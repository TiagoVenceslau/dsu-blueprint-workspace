{
    "plugins": [
        "plugins/markdown",
        "plugins/summarize",
        "node_modules/jsdoc-mermaid",
        "node_modules/better-docs/typescript",
        "node_modules/better-docs/category"
    ],
    "package": "package.json",
    "recurseDepth": 10,
    "source": {
        "include": [
            "."
        ],
        "exclude": [
            "./node_modules",
            "./cardinal",
            "./webcardinal",
            "./themes",
            "./privatesky",
            "./docs",
            "./workdocs",
            "./bin",
            "./tests",
            "./trust-loader-config",
            "./dossier-explorer-ssapp",
            "dossier-explorer-wallet-prototype",
            "apihub-root"
        ],
        "includePattern": "\\.(ts|tsx)$",
        "excludePattern": ".*/node_modules/.*"
    },
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "better-docs": {
            "name": "DSU Blueprint Workspace",
            "title": "OpenDSU Blueprint Workspace",
            "hideGenerator": true,
            "navLinks": [
                {
                    "label": "Github",
                    "href": "https://github.com/TiagoVenceslau/dsu-blueprint-workspace.git"
                }
            ]
        },
        "cleverLinks": false,
        "monospaceLinks": false,
        "search": true
    },
    "opts": {
        "encoding": "utf-8",
        "template": "./node_modules/better-docs",
        "destination": "./docs/",
        "readme": "./README.md",
        "recurse": true,
        "verbose": true
    }
}